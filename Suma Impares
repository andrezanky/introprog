{int N, Suma, I, I1;
 I=I1=1;
 printf("Ingrese numero Limite:");
 scanf("%f",&N);
 Suma=0;
 for(I=1;I<=N;I++)
 Suma = Suma + (2*I-1);
 printf("La suma de  los numeros impares es:%f", Suma);
  getch();
